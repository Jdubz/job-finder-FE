# Test Environment Variables
# Used for integration and E2E tests

# Firebase Configuration (Test Project or Emulator)
VITE_FIREBASE_API_KEY=test-api-key
VITE_FIREBASE_AUTH_DOMAIN=localhost
VITE_FIREBASE_PROJECT_ID=demo-test-project
VITE_FIREBASE_STORAGE_BUCKET=demo-test-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123456789:web:abcdef

# API Base URLs (Test Backend)
VITE_API_BASE_URL=http://localhost:5001/demo-test-project/us-central1

# Specific Function URLs (Test Backend)
VITE_GENERATOR_API_URL=http://localhost:5001/demo-test-project/us-central1/manageGenerator
VITE_EXPERIENCE_API_URL=http://localhost:5001/demo-test-project/us-central1/manageExperience
VITE_CONTENT_ITEMS_API_URL=http://localhost:5001/demo-test-project/us-central1/manageContentItems
VITE_JOB_QUEUE_API_URL=http://localhost:5001/demo-test-project/us-central1/manageJobQueue
VITE_CONTACT_FUNCTION_URL=http://localhost:5001/demo-test-project/us-central1/contact-form

# Firestore Database ID
VITE_FIRESTORE_DATABASE_ID=portfolio

# Emulator Configuration (for testing)
VITE_USE_EMULATORS=true
VITE_EMULATOR_HOST=localhost

# Build metadata
VITE_ENVIRONMENT=test

# Analytics (disabled for tests)
VITE_ENABLE_ANALYTICS=false

# Test User Credentials (for integration tests)
VITE_TEST_USER_EMAIL=test@example.com
VITE_TEST_USER_PASSWORD=testpassword123
VITE_TEST_EDITOR_EMAIL=editor@example.com
VITE_TEST_EDITOR_PASSWORD=editorpassword123
