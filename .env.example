# Environment Variables Example
# Copy this file to .env.development, .env.staging, or .env.production
# and fill in the values

# Firebase Configuration
VITE_FIREBASE_API_KEY=your-firebase-api-key
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your-messaging-sender-id
VITE_FIREBASE_APP_ID=your-app-id

# API Base URLs
VITE_API_BASE_URL=https://us-central1-your-project.cloudfunctions.net

# Specific Function URLs
VITE_GENERATOR_API_URL=https://us-central1-your-project.cloudfunctions.net/manageGenerator
VITE_EXPERIENCE_API_URL=https://us-central1-your-project.cloudfunctions.net/manageExperience
VITE_CONTENT_ITEMS_API_URL=https://us-central1-your-project.cloudfunctions.net/manageContentItems
VITE_JOB_QUEUE_API_URL=https://us-central1-your-project.cloudfunctions.net/manageJobQueue
VITE_CONTACT_FUNCTION_URL=https://us-central1-your-project.cloudfunctions.net/contact-form

# Firestore Database ID
VITE_FIRESTORE_DATABASE_ID=portfolio

# Emulator Configuration (for development only)
VITE_USE_EMULATORS=false
VITE_EMULATOR_HOST=localhost

# Build metadata
VITE_ENVIRONMENT=development

# Analytics
VITE_ENABLE_ANALYTICS=false
